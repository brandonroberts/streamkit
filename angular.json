{
  "version": 2,
  "newProjectRoot": "",
  "projects": {
    "ngtwitch": {
      "projectType": "application",
      "generators": {},
      "root": "apps/ngtwitch",
      "sourceRoot": "apps/ngtwitch/src",
      "prefix": "ngtwitch",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ngtwitch",
            "index": "apps/ngtwitch/src/index.html",
            "main": "apps/ngtwitch/src/main.ts",
            "polyfills": "apps/ngtwitch/src/polyfills.ts",
            "tsConfig": "apps/ngtwitch/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ngtwitch/src/favicon.ico",
              "apps/ngtwitch/src/assets",
              {
                "glob": "_redirects",
                "input": "apps/ngtwitch/src/configs",
                "output": "."
              }
            ],
            "styles": [
              "apps/ngtwitch/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ngtwitch/src/environments/environment.ts",
                  "with": "apps/ngtwitch/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "open": true,
            "proxyConfig": "apps/ngtwitch/src/proxy.conf.json"
          },
          "defaultConfiguration": "development",
          "configurations": {
            "production": {
              "browserTarget": "ngtwitch:build:production"
            },
            "development": {
              "browserTarget": "ngtwitch:build:development"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngtwitch:build"
          }
        },
        "test": {
          "executor": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/ngtwitch/src/test.ts",
            "polyfills": "apps/ngtwitch/src/polyfills.ts",
            "tsConfig": "apps/ngtwitch/tsconfig.spec.json",
            "karmaConfig": "apps/ngtwitch/karma.conf.js",
            "assets": [
              "apps/ngtwitch/src/favicon.ico",
              "apps/ngtwitch/src/assets"
            ],
            "styles": [
              "apps/ngtwitch/src/styles.css"
            ],
            "scripts": []
          },
          "outputs": [
            "coverage/apps/ngtwitch"
          ]
        },
        "lint": {
          "executor": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ngtwitch/tsconfig.app.json",
              "apps/ngtwitch/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngtwitch-e2e": {
      "projectType": "application",
      "root": "apps/ngtwitch-e2e",
      "targets": {
        "e2e": {
          "executor": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/ngtwitch-e2e/protractor.conf.js",
            "devServerTarget": "ngtwitch:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngtwitch:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/ngtwitch-e2e/tsconfig.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "generators": {},
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/api"
          ]
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "generators": {},
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/api-interfaces/**/*.ts"
            ]
          }
        }
      }
    },
    "actions": {
      "root": "libs/actions",
      "sourceRoot": "libs/actions/src",
      "projectType": "library",
      "generators": {},
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/actions/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/actions/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/actions"
          ]
        }
      }
    },
    "models": {
      "root": "libs/models",
      "sourceRoot": "libs/models/src",
      "projectType": "library",
      "generators": {},
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/models/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/models/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/models"
          ]
        }
      }
    },
    "tau": {
      "root": "tau",
      "sourceRoot": "tau/tau",
      "projectType": "application",
      "generators": {},
      "targets": {
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "tau",
            "command": "docker-compose up"
          }
        }
      }
    },
    "twitch-overlay": {
      "projectType": "application",
      "root": "apps/twitch-overlay",
      "sourceRoot": "apps/twitch-overlay/src",
      "prefix": "streamkit",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/twitch-overlay",
            "index": "apps/twitch-overlay/src/index.html",
            "main": "apps/twitch-overlay/src/main.ts",
            "polyfills": "apps/twitch-overlay/src/polyfills.ts",
            "tsConfig": "apps/twitch-overlay/tsconfig.app.json",
            "assets": [
              "apps/twitch-overlay/src/favicon.ico",
              "apps/twitch-overlay/src/assets"
            ],
            "styles": [
              "apps/twitch-overlay/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/twitch-overlay/src/environments/environment.ts",
                  "with": "apps/twitch-overlay/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "twitch-overlay:build:production"
            },
            "development": {
              "browserTarget": "twitch-overlay:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "twitch-overlay:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/twitch-overlay/src/**/*.ts",
              "apps/twitch-overlay/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/twitch-overlay"
          ],
          "options": {
            "jestConfig": "apps/twitch-overlay/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "twitch-overlay-e2e": {
      "root": "apps/twitch-overlay-e2e",
      "sourceRoot": "apps/twitch-overlay-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/twitch-overlay-e2e/cypress.json",
            "tsConfig": "apps/twitch-overlay-e2e/tsconfig.e2e.json",
            "devServerTarget": "twitch-overlay:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "twitch-overlay:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/twitch-overlay-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "generators": {
    "@schematics/angular:component": {
      "inlineStyle": true,
      "inlineTemplate": true
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "ngtwitch",
  "cli": {
    "defaultCollection": "@nrwl/angular"
  }
}